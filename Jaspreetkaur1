import java.util.*;
class Main {
    public static void main(String[] args) {
        int total=2;
        boolean arr[]=new boolean[4];
        queens(arr, total, 0, "");

    }
    public static void queens(boolean arr[],int total,int queenplaced,String ans){
        if(queenplaced==total){
            System.out.println(ans);
            return;
        }

        for(int i=0;i<arr.length;i++){
            if(arr[i]==false){
                arr[i]=true;
                queens(arr,total, queenplaced+1, ans+"B"+i+"Q"+queenplaced);
                arr[i]=false;
            }
        }
    }
}
